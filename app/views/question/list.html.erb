<h3><%= @user %>さん</h3>

<table border='1' class="table table-hover">
  <tr>
    <th>entity</th><th>attribute</th><th>value</th><th>flag</th>
  </tr>

<% @data.each do |datum| %>
  <tr>
    <td><%= datum.ent %></td>
    <td><%= datum.att %></td>
    <td><%= datum.val %></td>
    <td><%= datum.flag %></td>
  </tr>
<% end %>
</table>
