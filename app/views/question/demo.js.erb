  }
  <% @conf_a.each do |ca| %>
    $("#<%=ca%>").click(function(){
      $("#console").append("<%= Time.zone.now %><br><br>");
      $("#console").append("attributeを <%= ca %> に変更しました。<br><br>");
      $("#dialogue").append('<div class="well"><%= image_tag "pc.png", :size => "40x32", :align => "middle" %> &nbsp;&nbsp;<font size="3"><%= $a %> を <%= ca %> に変更しました。</font></div>');
